void imprimir_NombrePaises()
{
    for (int k=0;k<193;k++)
    {
        cout << k+1 << ". " << paises_del_mundo[k]
             << endl;
    }
}

int main() {
    cout << "Ejercicio 1 inciso c - Lista de Países" << endl;
    
    auto inicio = chrono::high_resolution_clock::now();
    
    imprimir_NombrePaises();
    
    auto fin = chrono::high_resolution_clock::now();

    auto duracion = chrono::duration_cast<chrono::microseconds>(fin - inicio);
    
    cout << "Tiempo de Impresion de paises: "
         << duracion.count() << " microsegundos"
         << endl;

    return 0;
}
